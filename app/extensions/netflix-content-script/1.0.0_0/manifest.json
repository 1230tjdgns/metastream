{
  "name": "Netflix Content Script Test",
  "version": "0.1",
  "description": "Content Script test",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["init.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://www.netflix.com/*"],
      "js": ["page.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "netflix-player.js"
  ],
  "manifest_version": 2
}
